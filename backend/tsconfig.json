{
  "compilerOptions": {
    "target": "ES6",
    "composite": true,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "outDir": "dist",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "include": ["./", "middlewares"],
  "exclude": ["node_modules", "dist"]
}
